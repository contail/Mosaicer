extends layout
block content
  .right_col(role='main')
    .col-md-12.col-sm-12.col-xs-12
      .x_panel
        .x_title
          h2
            | Market
          ul.nav.navbar-right.panel_toolbox
            li
              a.collapse-link
                i.fa.fa-chevron-up
            li
              a.close-link
                i.fa.fa-close
          .clearfix
        .x_content
          div
            .grid
            .grid-sizer
              - each data in datas
                  .grid-item.w2
                    .hovereffect
                      img(src='util/img?folder=#{data.path}&file=#{data.name}')
                      .overlay
                        h2 Effect 12
                        p.set1
                          a(href='#')
                            i.fa.fa-twitter
                          a(href='#')
                            i.fa.fa-facebook
                        hr
                        hr
                        p.set2
                          a(href='#')
                            i.fa.fa-instagram
                          a(href='#')
                            i.fa.fa-dribbble
    script.
      getMarket()
